#!/usr/bin/perl
	$fred[0]="Hello";
	$fred[1]="I'm a nerd!";
	$number = 0;
	print $fred[1]."\n";
	print $fred[$number]."\n";

	$fred[2] = "we are J";
	$fred[4] = "again";
	$string = $#fred;
	print $string."\n";
	print $fred[-1]."\n";

	($liu,$Zhang) = qw(nan nv);
	print $liu." ".$Zhang."\n";
	($liu,$Zhang) = qw(nan);
	print $liu." ".$Zhang."\n";

	$Pop_string = pop(@fred);
	print $Pop_string."\n";
	print @fred;
	print "\n";

	@array = qw#I'm haunary baby!#;
	$m = shift(@array);
	$n = shift(@array);
	print $m."\n";
	print $n."\n";
	print @array;
	print "\n";

	unshift(@array,4);
	unshift(@array,0);
	print @array;
	print "\n";

	@fred2 = qw(hello dolly);
	$y = 2 * 4;
	$x = "This is $fred2[1]'s palce";
	print $x,"\n";
	$x = "This is $fred2[$y-1]'s place";
	print $x."\n";
